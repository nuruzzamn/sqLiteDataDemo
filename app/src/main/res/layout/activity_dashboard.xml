<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffd8a3"
    tools:context="sqlite.example.com.sqlitedata.DashboardActivity">

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="73dp"
        android:text="Congratulation, You Have Completed Your Registration Successfully"
        tools:layout_editor_absoluteX="161dp"
        tools:layout_editor_absoluteY="79dp"
        android:gravity="center"
        android:textSize="23sp"
        android:textStyle="bold|italic"
        android:textColor="#f96103"/>

    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView6"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="43dp"
        android:gravity="center"
        android:textSize="20sp"/>

    <Button
        android:id="@+id/button1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView1"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="105dp"
        android:text="Log Out"
        android:textSize="20sp"
        android:textStyle="bold"
        android:textColor="#ff0f27"
        />
</RelativeLayout>
